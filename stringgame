#include <string.h>
#include <stdio.h>
#include <ctype.h>
#include <stdlib.h>
#include <math.h>
#include <limits.h>
void f_writte(char[]);
void f_writte_end(char[]);

int main(){
    char c[25];
    scanf("%[^\n]*c",c);
    f_writte(c);
    f_writte_end(c);
    
    return 0;
}
void f_writte(char c1[]){
    
    for(int i = 0 ; i<strlen(c1) ; i++){
        for( int j = 0; j<=i; j++){
            printf("%c",c1[j]);
        }
        printf("\n");
    }
}
void f_writte_end(char c1[]){
    long len =strlen(c1);
    for(long i= len-1 ; i>=0;  i--){
        for(long  j=0  ; j<=i ; j++){
            printf("%c",c1[j]);
            
        }
        printf("\n");
    }
    
}
